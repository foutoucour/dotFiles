# -*- coding: utf-8 -*-
import lm__Namer
from PyQt4 import QtGui
from PyQt4 import QtCore

class Model(QtGui.QWidget):
    def getReferenceName(self):
        self.__sRefName=self.__oNamer.getValue()
        print self.__oNamer, 'from model :', self.__sRefName
        return self.__sRefName
                
    def setName(self,string):
        def __emitSignal():
            self.emit(QtCore.SIGNAL('Model_nameSet'))
            
        self.__oNamer = lm__Namer.Naming(string)
        self.__oNamer.show()
        self.connect(self.__oNamer,
                     QtCore.SIGNAL('Naming_nameSet'),
                     __emitSignal)

